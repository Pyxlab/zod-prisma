{
	"name": "@pyxlab/zod-prisma",
	"version": "0.6.1",
	"private": false,
	"description": "A Prisma generator that creates Zod schemas for all of your models",
	"license": "MIT",
	"author": "Walaff Fernandes",
	"homepage": "https://github.com/Pyxlab/zod-prisma#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Pyxlab/zod-prisma.git"
	},
	"bugs": {
		"url": "https://github.com/Pyxlab/zod-prisma/issues"
	},
	"main": "dist/index.js",
	"module": "dist/zod-prisma.esm.js",
	"typings": "dist/index.d.ts",
	"bin": {
		"zod-prisma": "bin/cli.js"
	},
	"keywords": [
		"zod",
		"prisma",
		"generator"
	],
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"build": "dts build --target node --format cjs --rollupTypes",
		"lint": "tsc --noEmit && dts lint src --ignore-pattern src/test/functional",
		"prepare": "husky install",
		"prepublish": "dts build --target node --format cjs --rollupTypes",
		"start": "dts watch",
		"test": "dts test --maxWorkers=4 --verbose"
	},
	"prettier": {
		"printWidth": 100,
		"semi": false,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "es5",
		"useTabs": true
	},
	"eslintConfig": {
		"rules": {
			"react-hooks/rules-of-hooks": "off"
		}
	},
	"jest": {
		"testEnvironment": "node"
	},
	"dependencies": {
		"@prisma/generator-helper": "~5.9.1",
		"lodash.uniq": "^4.5.0",
		"parenthesis": "^3.1.8",
		"ts-morph": "^20.0.0"
	},
	"devDependencies": {
		"@prisma/client": "~5.6.0",
		"@prisma/sdk": "^4.0.0",
		"@tsconfig/recommended": "^1.0.3",
		"@types/fs-extra": "^11.0.4",
		"@types/lodash.uniq": "^4.5.9",
		"dts-cli": "^2.0.3",
		"execa": "^5.1.0",
		"fast-glob": "^3.3.2",
		"fs-extra": "^11.1.1",
		"husky": "^8.0.3",
		"jest-mock-extended": "^3.0.5",
		"prisma": "^5.6.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"zod": "^3.22.4"
	},
	"peerDependencies": {
		"decimal.js": "^10.4.3",
		"prisma": "^5.6.0",
		"zod": "^3.22.4"
	},
	"peerDependenciesMeta": {
		"decimal.js": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=14"
	},
	"publishConfig": {
		"tag": "latest",
		"access": "public"
	}
}
